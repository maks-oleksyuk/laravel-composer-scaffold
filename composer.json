{
    "name": "maks-oleksyuk/laravel-composer-scaffold",
    "description": "A flexible Composer scaffold builder for Laravel project.",
    "license": "GPL-3.0-or-later",
    "type": "composer-plugin",
    "version": "1.0.x-dev",
    "keywords": [
        "laravel",
        "plugin",
        "scaffold"
    ],
    "authors": [
        {
            "name": "Maks Oleksyuk",
            "email": "maksym.oleksyuk@gmail.com"
        }
    ],
    "homepage": "https://github.com/maks-oleksyuk/laravel-composer-scaffold",
    "require": {
        "php": "^8.2",
        "composer-plugin-api": "^2"
    },
    "require-dev": {
        "composer/composer": "^2.7",
        "laravel/pint": "^1.18"
    },
    "autoload": {
        "psr-4": {
            "MaksOleksyuk\\Composer\\Plugin\\LaravelScaffold\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "class": "MaksOleksyuk\\Composer\\Plugin\\LaravelScaffold\\Plugin"
    }
}
